#include <stdio.h>
void bubbleSort(int arr[], int n)
{
    for (int i = 0; i < n - 1; i++) 
    {
        for (int j = 0; j < n - i - 1; j++)
        {
            if (arr[j] > arr[j + 1]) 
            {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}
void main() 
{
    int n;
    printf("Enter the number of students: ");
    scanf("%d", &n);
    int marks[n];
    printf("Enter the marks of %d students:\n", n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &marks[i]);
    }
    bubbleSort(marks, n);
    printf("Sorted marks in ascending order: ");
    for (int i = 0; i < n; i++) 
    {
        printf("%d ", marks[i]);
    }
}
