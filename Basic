#include<stdio.h>
void main()
{
    int a,b;
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        printf("Maximum number is A");
    }
    else
    {
        printf("Maximum number is B");
    }
}

#include <stdio.h>
void main()
{
    int a; 
    scanf("%d",&a);
    if (a>0)
    {
        printf("It is Positive");
    }
    else if (a==0)
    {
        printf("It is Zero");
    }
    else
    {
        printf("It is Negative");
    }
}

#include <stdio.h>
void main()
{
    int n;
    scanf("%d",&n);
    if(n%2)
    {
        printf("It's Odd number");
    }
    else
    {
        printf("It's Even number");
    }
}

#include <stdio.h>
void main()
{
    int y; 
    scanf("%d",&y);
    if((y % 4 == 0 && y % 100 != 0) || (y % 400 == 0))
    {
        printf("It's Leap year");
    }
    else
    {
        printf("It's not Leap year");
    }
}
